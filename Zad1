class School(object):
  def __init__(self, list_of_classes)
    self.list_of_classes = list_of_classes

class Class(object):
  average_score = 0;
  def __init__(self, class_number, list_of_students)
    self.class_number = class_number
    self.list_of_students = list_of_students

 
class Student(Object):
  def __init__(self, name, surname, class_number, score):
    self.name = name
    self.surname = surname
    self.class_number = class_number
    self.score = score

